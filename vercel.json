{
    "version": 2,
    "public": true,
    "headers": [
      {
        "source": "/(.*).pdf",
        "headers": [
          { 
            "key": "Content-Type", 
            "value": "application/pdf" 
          },
          { 
            "key": "Content-Disposition", 
            "value": "attachment; filename=\"Shreya_Srivastava_Resume.pdf\"" 
          },
          { 
            "key": "Cache-Control", 
            "value": "public, max-age=31536000, immutable" 
          }
        ]
      }
    ],
    "rewrites": [
      {
        "source": "/Shreya_Resume.pdf",
        "destination": "/Shreya_Resume.pdf"
      }
    ],
    "routes": [
      {
        "handle": "filesystem"
      },
      {
        "src": "/.*",
        "dest": "/index.html"
      }
    ]
  }